         	// 	}
         	/********************************/
         	/*** calculating duration time **/
         	/********************************/
         	duration_time = startTime.timeBetween(endTime);
         	lis = duration_time.toList(":");
         	lis = lis.get(0).leftPad(2).replaceAll(" ","0") + ":" + lis.get(1).leftPad(2).replaceAll(" ","0");
         	duration_time = lis.toString();
         	return duration_time;
         }
